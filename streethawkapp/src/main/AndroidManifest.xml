<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.streethawk.streethawkapp">


    <permission
        android:name="com.streethawk.streethawkapp.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.streethawk.streethawkapp.permission.C2D_MESSAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter android:label="activity">
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="activity"
                    android:scheme="streethawkapp" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FlashScreen"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Analytics"
            android:label="@string/title_activity_analytics"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.streethawk.streethawkapp.MainActivity" />
        </activity>
        <receiver
            android:name="com.streethawk.library.push.GCMReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <!--Add  package_name here-->
                <category android:name="com.streethawk.streethawkapp" />
            </intent-filter>
        </receiver>
        <activity
            android:name=".StartBGL"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.streethawk.streethawkapp.MainActivity" />
        </activity>
        <activity
            android:name=".Setting"
            android:label="@string/title_activity_settings"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.streethawk.streethawkapp.MainActivity" />
        </activity>
        <activity android:name=".SetupActivity" />
        <activity
            android:name=".GrowthActivity"
            android:label="@string/title_activity_growth"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.streethawk.streethawkapp.MainActivity" />
        </activity>
        <activity
            android:name=".PushActivity"
            android:label="@string/title_activity_push"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.streethawk.streethawkapp.MainActivity" />

            <intent-filter android:label="activity">
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="push"
                    android:scheme="streethawkapp" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Feedback"
            android:label="@string/title_activity_feedback"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.streethawk.streethawkapp.MainActivity" />
        </activity>

        <receiver
            android:name=".FeedBroadCastReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.streethawk.intent.action.newfeed" />
            </intent-filter>
        </receiver>
        <!--
        <service
            android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>


        <service
            android:name=".MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
        -->
        <service
            android:name=".TestService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".FeedViewerActivity"
            android:label="@string/title_activity_feed_viewer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FeedList"
            android:label="@string/title_activity_feed_list"
            android:theme="@style/AppTheme.NoActionBar">
        <intent-filter android:label="FeedList">
            <action android:name="android.intent.action.VIEW" />

            <category android:name="android.intent.category.BROWSABLE" />
            <category android:name="android.intent.category.DEFAULT" />

            <data
                android:host="feedlist"
                android:scheme="streethawkapp" />
        </intent-filter>
        </activity>

        <service
            android:name=".SHFabService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".WebViewPOC"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_web_view_poc"
            android:theme="@style/FullscreenTheme" />
        <activity
            android:name=".Logreport"
            android:label="@string/title_activity_logreport"
            android:theme="@style/AppTheme.NoActionBar" />

    </application>

</manifest>