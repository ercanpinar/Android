<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.streethawk.library.geofence">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name">
        <uses-library android:name="com.google.android.maps" />

        <service
            android:name=".GeofenceService"
            android:exported="false"></service>

        <receiver
            android:name=".SHCoreModuleReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.streethawk.intent.action.APP_STATUS_NOTIFICATION" />
                <action android:name="android.location.PROVIDERS_CHANGED" />
            </intent-filter>
        </receiver>

        <activity android:name=".AskGeoPermission"
            android:theme="@style/Theme.AppCompat.Streethawk_Translucent"/>
    </application>

</manifest>
